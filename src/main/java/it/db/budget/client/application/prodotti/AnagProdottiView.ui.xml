<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:d="urn:import:org.gwtbootstrap3.demo.client.ui">

	<b:Panel>
		<b:PanelHeader>
			<b:Heading size="H3" text="Anagrafica Prodotti" />
		</b:PanelHeader>
		<b:PanelBody>
			<b:Form>
				<b:FieldSet>
					<b:FormGroup>
      					<b:FormLabel for="formProdotto">Prodotto</b:FormLabel>
      					<b:TextBox placeholder="Prodotto..." b:id="formProdotto" ui:field="prodottoTB"/>
    				</b:FormGroup>
    				<b:Button type="PRIMARY" ui:field="cercaButton">Cerca</b:Button>
    				<b:Button type="PRIMARY" ui:field="salvaButton">Salva</b:Button>
    				<b.gwt:CellTable bordered="true" condensed="true" striped="true" hover="true" height="300px" width="100%" ui:field="cellTable"/>
				</b:FieldSet>
			</b:Form>
			
		</b:PanelBody>
	</b:Panel>
</ui:UiBinder>
